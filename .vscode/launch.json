{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--inspect",
        "--folder-uri=${workspaceRoot}/packages/tencent-mars-xlog-rust/sample_data",
        "--extensionDevelopmentPath=${workspaceRoot}"
      ],
      "outFiles": ["${workspaceRoot}/dist/client.js"]
    }
  ]
}
